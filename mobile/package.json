{
  "name": "silhouette-workflow-mobile",
  "version": "1.0.0",
  "description": "Silhouette Workflow Creation - Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "android:debug": "react-native run-android --variant=debug",
    "android:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SilhouetteWorkflow.xcworkspace -scheme SilhouetteWorkflow archive"
  },
  "keywords": [
    "workflow",
    "automation",
    "react-native",
    "mobile",
    "expo"
  ],
  "author": "Silhouette Team",
  "license": "MIT",
